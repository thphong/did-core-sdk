{
  "name": "did-core-sdk",
  "version": "1.0.3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.js",
    "./dist/index.js": "./dist/index.browser.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.browser.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch --onSuccess \"node debug/run.js\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build && npm run typecheck"
  },
  "repository": {
    "type": "git",
    "url": "https://phd3hc@dev.azure.com/phd3hc/did-core-sdk/_git/did-core-sdk"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@iota/identity-wasm": "^1.6.0-beta.10",
    "@iota/iota-sdk": "^1.6.1",
    "@iota/sdk-wasm": "^2.0.0-beta.1",
    "@types/libsodium-wrappers": "^0.7.14",
    "libsodium-wrappers": "^0.7.15"
  }
}
